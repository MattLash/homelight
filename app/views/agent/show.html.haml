.agent_show_container
	.container
		.agent_show_inner
			.agent_photo= image_tag "#{@agent.photo_url}"
			%h1= @agent.name
			%h3 Brokerage: #{@agent.brokerage}
			%p Buyers: #{ @agent.agent_stat.buyers}
			%p Sellers: #{ @agent.agent_stat.sellers}
			%p SFH: #{ @agent.agent_stat.sfh}
			%p Condo: #{ @agent.agent_stat.condo}
			%p townhome: #{ @agent.agent_stat.townhome}
			%p mobile: #{ @agent.agent_stat.mobile}


